@model List<Menu>


 @foreach(var menu in Model)
{
    <div class="card" style="width:18rem">
        <div class="card-body">
            <h5 class="card-title">@menu.DishName</h5>
            <p class="card-text">@menu.Price</p>  
            <h7>@menu.Description</h7>
            <h7>Is Available : @menu.IsAvailable</h7>
        </div>
    </div>
   
    
}
  